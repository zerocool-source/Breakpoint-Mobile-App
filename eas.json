{
  "cli": {
    "version": ">= 16.31.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "API_URL": "https://breakpoint-api-v2.onrender.com",
        "TECHOPS_URL": "https://breakpoint-app.onrender.com"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "API_URL": "https://breakpoint-api-v2.onrender.com",
        "TECHOPS_URL": "https://breakpoint-app.onrender.com"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "API_URL": "https://breakpoint-api-v2.onrender.com",
        "TECHOPS_URL": "https://breakpoint-app.onrender.com"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
